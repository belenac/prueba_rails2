<div class="container">
  <h3>ToDo</h3>
  <p><%= @todo.name %></p>
  <ul>
  <p>Usuarios que han completado la tarea</p>
  <% @s.each do |u| %>
      <li><%= u.user.email %></li>
  <% end %>

  <p>Usuarios que han agregado la tarea</p>
  <% @todo.users.each do |u| %>
      <li><%= u.email %></li>
  <% end %>
  </ul>
  <%= link_to  "Volver", todos_path, class:'btn btn-default' %>
</div>